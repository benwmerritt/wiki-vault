  "customRegexes": [
    {
      "label": "Remove Perplexity logo HTML tag",
      "find": "<img src=\"https://r2cdn\\.perplexity\\.ai/[^\"]+\"[^>]*/>\\n*",
      "replace": "",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Remove hidden footnote spans",
      "find": "<span style=\"display:none\">[^<]+</span>\\n*",
      "replace": "",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Replace HTML ornamental divider with markdown",
      "find": "<div style=\"text-align: center\">⁂</div>",
      "replace": "---",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Clean up duplicate References headings",
      "find": "(^#{1,2} References\\n+){2,}",
      "replace": "## References\\n\\n",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Add References heading if missing",
      "find": "^((?:(?!## References)[\\s\\S])*?)(\\[\\^1\\]:.*)$",
      "replace": "$1## References\\n\\n$2",
      "flags": "m",
      "enabled": true
    },
    {
      "label": "Add research topic line after H1",
      "find": "^(# (.+))\\n(?!>  \\*\\*Research Topic\\*\\*)(.)",
      "replace": "$1\\n>  **Research Topic**: [[$2]]\\n---\\n$3",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "Footnote: unnest definitions from lists/quotes",
      "find": "^\\s*(?:>+\\s*|(?:[-*]|\\d+[.)])\\s*)\\[\\^(\\d+)\\]:\\s*(.+)$",
      "replace": "[^$1]: $2",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote (numeric): inline refs to caret",
      "find": "(?<!\\^)(?<!\\[)\\[(\\d+)\\](?=\\s|[,.;!?)]|\\[|$)(?!\\()",
      "replace": "[^$1]",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote (numeric): bare URL to definition",
      "find": "^\\s*(?:[-*]\\s*)?\\[(\\d+)\\](?!:)\\s*.*?(https?://\\S+)\\s*$",
      "replace": "[^$1]: $2",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote (numeric): parentheses to definition",
      "find": "^\\s*(?:[-*]\\s*)?\\[(\\d+)\\]\\(([^)]+)\\)\\s*$",
      "replace": "[^$1]: $2",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote (numeric): convert numeric definitions to caret",
      "find": "^\\s*\\[(\\d+)\\]:\\s*(.+)$",
      "replace": "[^$1]: $2",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote: inline bare URL to definition",
      "find": "^\\s*(?:[-*]\\s*)?\\[\\^(\\d+)\\](?!:)\\s*.*?(https?://\\S+)\\s*$",
      "replace": "[^$1]: $2",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote: inline parentheses to definition",
      "find": "^\\s*(?:[-*]\\s*)?\\[\\^(\\d+)\\]\\(([^)]+)\\)\\s*$",
      "replace": "[^$1]: $2",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote: strip trailing parenthesis in definitions",
      "find": "^\\s*(\\[\\^\\d+\\]:\\s*https?://\\S+)\\)\\s*$",
      "replace": "$1",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote fix",
      "find": "^\\s*\\[\\^\\d+\\]:\\s*$",
      "replace": "",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Footnote: remove blank lines between definitions",
      "find": "^(\\s*\\[\\^\\d+\\]:[^\\n]*)\\n(?:\\s*\\n)+(?=\\s*\\[\\^\\d+\\]:)",
      "replace": "$1\\n",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Remove blank line after heading",
      "find": "^(#{2,6} .+)\\n\\n",
      "replace": "$1\\n",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "Remove all multiple blank lines",
      "find": "\\n{3,}",
      "replace": "\\n\\n",
      "flags": "gm",
      "enabled": false
    },
    {
      "label": "Remove blank line after HR before heading",
      "find": "^(-{3,}|\\*{3,}|_{3,})\\n\\n(#{1,6} .+)$",
      "replace": "$1\\n$2",
      "flags": "gm",
      "enabled": true
    },
    {
      "label": "Remove blank line between blockquote and horizontal rule",
      "find": "^(>.*?)\\n\\n(---|\\*\\*\\*|___)$",
      "replace": "$1\\n$2",
      "flags": "gm",
      "enabled": true
    }
  ],
  "commonStyles": {
